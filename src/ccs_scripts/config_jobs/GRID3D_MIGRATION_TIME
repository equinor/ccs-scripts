EXECUTABLE grid3d_migration_time

DEFAULT <ECLROOT> -1
DEFAULT <MAPFOLDER> -1
DEFAULT <PLOTFOLDER> -1
DEFAULT <FOLDERROOT> -1
DEFAULT <NO_LOGGING> -1
DEFAULT <DEBUG> -1

ARGLIST "--config" <CONFIG_MIGTIME> "--eclroot" <ECLROOT> "--mapfolder" <MAPFOLDER> "--plotfolder" <PLOTFOLDER> "--folderroot" <FOLDERROOT> "--no_logging" <NO_LOGGING> "--debug" <DEBUG>

STDERR GRID3D_MIGRATION_TIME.stderr
STDOUT GRID3D_MIGRATION_TIME.stdout

MIN_ARG 1
MAX_ARG 7