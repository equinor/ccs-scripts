EXECUTABLE grid3d_aggregate_map

DEFAULT <ECLROOT> -1
DEFAULT <MAPFOLDER> -1
DEFAULT <PLOTFOLDER> -1
DEFAULT <FOLDERROOT> -1

ARGLIST "--config" <CONFIG_AGGREGATE> "--eclroot" <ECLROOT> "--mapfolder" <MAPFOLDER> "--plotfolder" <PLOTFOLDER> "--folderroot" <FOLDERROOT>

STDERR GRID3D_AGGREGATE_MAP.stderr
STDOUT GRID3D_AGGREGATE_MAP.stdout

MIN_ARG 1
MAX_ARG 5
